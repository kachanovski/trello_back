{
  "openapi": "3.0.0",
  "info": {
    "title": "Trello Clone API",
    "version": "v1",
    "description": "header === body"
  },
  "servers": [
    {
      "url": "https://trello-app-clone.herokuapp.com/"
    },
    {
      "url": "http://localhost:5000/"
    }
  ],
  "tags": [
    {
      "name": "auth"
    },
    {
      "name": "boards"
    }
  ],
  "paths": {
    "/auth/login": {
      "post": {
        "summary": "login",
        "tags": [
          "auth"
        ],
        "parameters": [
          {
            "name": "data",
            "in": "header",
            "required": true,
            "description": "Data for Login",
            "schema": {
              "type": "object",
              "example": {
                "email": "string",
                "password": "string",
                "rememberMe": false
              }
            }
          }
        ],
        "description": "Parameters",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "object",
                  "example": {
                    "resultCode": 0,
                    "data": {
                      "email": "string",
                      "userName": "string || ===email"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/auth/register": {
      "post": {
        "summary": "register",
        "tags": [
          "auth"
        ],
        "parameters": [
          {
            "name": "data",
            "in": "header",
            "required": true,
            "description": "data for register",
            "schema": {
              "example": {
                "email": "string",
                "password": "string"
              }
            }
          }
        ],
        "description": "Parameters",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "string",
                  "example": {
                    "resultCode": 0,
                    "message": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/auth/me": {
      "get": {
        "summary": "authMe",
        "tags": [
          "auth"
        ],
        "description": "Parameters",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "object",
                  "example": {
                    "resultCode": 0,
                    "data": {
                      "id": "string",
                      "email": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/auth/logout": {
      "delete": {
        "summary": "logOut",
        "tags": [
          "auth"
        ],
        "description": "Parameters",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "object",
                  "example": {
                    "resultCode": 0,
                    "message": "string"
                  }
                }
              }
            }
          }
        }
      }
    },


    "/boards": {
      "get": {
        "summary": "allBoards",
        "tags": [
          "boards"
        ],
        "description": "Parameters",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "object",
                  "example": {
                    "resultCode": 0,
                    "data": {
                      "board": "Array Boards"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "addBoard",
        "tags": [
          "boards"
        ],
        "parameters": [
          {
            "name": "boardData",
            "in": "path",
            "required": true,
            "description": "board data",
            "schema": {
              "type": "object",
              "example": {
                "title": "string",
                "author_id": "string"
              }
            }
          }
        ],
        "description": "Parameters",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "object",
                  "example": {
                    "resultCode": 0,
                    "message": "Board added"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/boards/{id}": {
      "get": {
        "summary": "findBoard",
        "tags": [
          "boards"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "board id",
            "schema": {
              "type": "string",
              "example": {
                "id": "string"
              }
            }
          }
        ],
        "description": "Parameters",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "object",
                  "example": {
                    "resultCode": 0,
                    "data": {
                      "board": {
                        "id": "string",
                        "title": "string",
                        "author_id": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "updateBoard",
        "tags": [
          "boards"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "board id",
            "schema": {
              "type": "string",
              "example": {
                "id": "string"
              }
            }
          },
          {
            "name": "board",
            "in": "header",
            "required": true,
            "description": "board data",
            "schema": {
              "type": "object",
              "example": {
                "title": "string"
              }
            }
          }
        ],
        "description": "Parameters",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "object",
                  "example": {
                    "resultCode": 0,
                    "data": {
                      "board": {
                        "id": "string",
                        "title": "string",
                        "author_id": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
        "summary": "deleteBoard",
        "tags": [
          "boards"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "board id",
            "schema": {
              "type": "string",
              "example": {
                "id": "string"
              }
            }
          }
        ],
        "description": "Parameters",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "object",
                  "example": {
                    "resultCode": 0,
                    "message": "board deleted"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "rememberMe": {
            "type": "boolean"
          },
          "teams": {
            "type": "array"
          },
          "boards": {
            "type": "array"
          },
          "verified": {
            "type": "boolean"
          },
          "avatar": {
            "type": "string"
          },
          "token": {
            "type": "string"
          },
          "tokenDeathTime": {
            "type": "number"
          },
          "resetPasswordToken": {
            "type": "string"
          },
          "resetPasswordTokenDeathTime": {
            "type": "number"
          }
        }
      },
      "Board": {
        "type": "object",
        "properties": {
          "author_id": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "created": {
            "type": "string"
          }
        }
      }
    }
  }
}